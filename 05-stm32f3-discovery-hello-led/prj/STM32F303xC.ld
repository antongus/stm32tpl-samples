/**
 * linker script for STM32F303xC 
 */ 

MEMORY
{
	RAM (xrw)	: ORIGIN = 0x20000000, LENGTH =   40K       /* Main RAM (can be accessed by DMA) */
	FLASH (rx)	: ORIGIN = 0x08000000, LENGTH =  256K       /* Flash */
	BATTRAM (rw): ORIGIN = 0x40024000, LENGTH =    0K       /* Battery backed RAM */
	CCRAM (rw)	: ORIGIN = 0x10000000, LENGTH =    8K       /* Core coupled RAM (can NOT be accessed by DMA) */
}

INCLUDE "stm32.ld"
